{
  "name": "xipasduarte/wp-post-exporter",
  "description": "Export any WordPress post type to CVS.",
  "version": "1.0.1",
  "type": "wordpress-plugin",
  "keywords": ["wordpress", "plugin", "export", "cvs"],
  "homepage": "https://github.com/xipasduarte/wp-post-exporter",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Pedro Duarte",
      "email": "xipasduarte@gmail.com",
      "homepage": "https://github.com/xipasduarte"
    }
  ],
  "support": {
    "issues": "https://github.com/xipasduarte/wp-post-exporter/issues"
  },
  "require": {
    "php": ">=7.0.10",
    "composer/installers": "~1.0",
    "league/csv": "^9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "*@stable",
    "10up/wp_mock": "@dev"
  },
  "autoload": {
    "psr-4": {
      "xipasduarte\\WP\\Plugin\\PostExporter\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "xipasduarte\\WP\\Plugin\\PostExporter\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "config": {
    "sort-packages": true
  }
}
